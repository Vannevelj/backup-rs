    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/backup-rs.exe",
            "args": ["C:\\Users\\jer_v\\Pictures\\Big Dump of Pictures", "--bucket", "zenzizenzi-photography"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'backup-rs'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=backup-rs",
                    "--package=backup-rs"
                ],
                "filter": {
                    "name": "backup-rs",
                    "kind": "bin"
                }
            },
            "args": ["~/Documents/Pictures", "--bucket", "zenzizenzi-photography"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'backup-rs'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=backup-rs",
                    "--package=backup-rs"
                ],
                "filter": {
                    "name": "backup-rs",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}